  "React Imperative Modal Component": {
    "prefix": "!rcimpantdmodal",
    "description": "useImperativeHandle 을 이용한 명령형 기능이 포함된 모달 컴포넌트를 만든다.",
    "body": [
      "import { ReactNode, forwardRef, useImperativeHandle, useState, useRef } from 'react';",
      "import { Button, Modal, Popconfirm, Space } from 'antd';",
      "import styled from 'styled-components';",
      "import { PromiseResolverDto } from '@common/models';",
      "",
      "interface ${1:${TM_FILENAME_BASE}Props} {",
      "  defaultShow?: boolean;",
      "  children?: ReactNode;",
      "}",
      "",
      "export interface ${2:${TM_FILENAME_BASE}Handle} {",
      "  open(): Promise<${4:void}>",
      "}",
      "",
      "const ${3:Wrap} = styled.div`",
      "",
      "`;",
      "",
      "export const $TM_FILENAME_BASE = forwardRef<",
      "  ${2},",
      "  ${1}",
      ">(({",
      "  defaultShow = false,",
      "  children,",
      "  ...props,",
      "}, ref) => {",
      "  const refResolver = useRef<PromiseResolverDto<${4:void}> | null>(null);",
      "  const [show, setShow] = useState(defaultShow);",
      "",
      "  useImperativeHandle(",
      "    ref,",
      "    () => ({",
      "      open() {",
      "        return new Promise<${4:void}>((resolve, reject) => {",
      "          setShow(true);",
      "          refResolver.current = {",
      "            resolve,",
      "            reject,",
      "          };",
      "        });",
      "      },",
      "    }),",
      "    [],",
      "  );",
      "",
      "  const handleCancel = () => {",
      "    setShow(false);",
      "  ",
      "    if (refResolver.current) {",
      "      refResolver.current.reject(null);",
      "      refResolver.current = null;",
      "    }",
      "  };",
      "",
      "  const handleOk = () => {",
      "    setShow(false);",
      "  ",
      "    if (refResolver.current) {",
      "      refResolver.current.resolve();",
      "      refResolver.current = null;",
      "    }",
      "  };",
      "",
      "  return (",
      "    <Modal",
      "      title=\"제목\"",
      "      visible={show}",
      "      cancelText=\"취소\"",
      "      okText=\"확인\"",
      "      onCancel={handleCancel}",
      "      onOk={handleOk}",
      "    >",
      "      {children}",
      "    </Modal>",
      "  );",
      "});",
      "",
      "$TM_FILENAME_BASE.displayName = '$TM_FILENAME_BASE';",
      ""
    ]
  },
