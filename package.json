{
  "name": "react-new-template",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "start": "vite",
    "start:dev": "env-cmd -f .env.development vite",
    "start:prod": "env-cmd -f .env.production vite",
    "build": "tsc --noEmit && vite build",
    "build:dev": "tsc --noEmit && env-cmd -f env.development vite build",
    "build:prod": "tsc --noEmit && env-cmd -f env.development vite build",
    "serve": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "cli": "jordy-cli",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "sb": "start-storybook -p 6006",
    "build:sb": "build-storybook",
    "serve:sb": "npx http-server ./storybook-static -o",
    "cy": "cypress open",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.4",
    "axios": "^0.27.2",
    "jordy": "^0.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-vite": "^0.2.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/node": "^18.7.2",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-legacy": "^2.0.1",
    "@vitejs/plugin-react": "^2.0.1",
    "babel-loader": "^8.2.5",
    "cypress": "^10.4.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.4",
    "jordy-cli": "^0.4.1",
    "jsdom": "^20.0.0",
    "prettier": "^2.7.1",
    "terser": "^5.14.2",
    "typescript": "^4.7.4",
    "vite": "^3.0.6",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.21.1"
  },
  "author": "",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
