{
  "name": "react-new-template",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "start": "vite",
    "start:dev": "env-cmd -f .env.development vite",
    "start:prod": "env-cmd -f .env.production vite",
    "build": "tsc --noEmit && vite build",
    "build:dev": "tsc --noEmit && env-cmd -f .env.development vite build",
    "build:prod": "tsc --noEmit && env-cmd -f .env.production vite build",
    "serve": "vite preview",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "cli": "jordy-cli",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "sb": "storybook dev -p 6006",
    "build:sb": "storybook build",
    "serve:sb": "npx http-server ./storybook-static -o",
    "cy": "cypress open",
    "cy:run": "cypress run"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
